CREATE TABLE "Users" ("Id" TEXT PRIMARY KEY  NOT NULL  UNIQUE , "UserName" TEXT NOT NULL  UNIQUE , "PasswordHash" TEXT NOT NULL , "LastActivity" DATETIME, "Token" TEXT)
CREATE TABLE "Messages" ("Id" TEXT PRIMARY KEY NOT NULL UNIQUE, "SenderId" TEXT NOT NULL, "RecipientId" TEXT NOT NULL, "PostDate" DATETIME NOT NULL, "Body" TEXT NOT NULL, "IsRead" BOOL NOT NULL);